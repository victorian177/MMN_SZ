issn = {15386724},
journal = {Physical Therapy},
month = {mar},
number = {3},
pages = {415--425},
pmid = {25212522},
publisher = {American Physical Therapy Association},
title = {{Emergence of virtual reality as a tool for upper limb rehabilitation: Incorporation of motor control and motor learning Principles}},
