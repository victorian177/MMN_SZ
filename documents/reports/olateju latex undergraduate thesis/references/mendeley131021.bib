file = {:D$\backslash$:/mendeley/Waldner, Tomelleri, Hesse/Functional Neurology/Waldner, Tomelleri, Hesse{\_}2009{\_}Transfer of scientific concepts to clinical practice Recent robot-assisted training studies.pdf:pdf},
