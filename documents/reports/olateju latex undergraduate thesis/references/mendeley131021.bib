author = {Hung, Chung Shan and Hsieh, Yu Wei and Wu, Ching Yi and Chen, Yu Ju and Lin, Keh Chung and Chen, Chia Ling and Yao, Kaiping Grace and Liu, Chien Ting and Horng, Yi Shiung},
doi = {10.1177/1539449218825438},
