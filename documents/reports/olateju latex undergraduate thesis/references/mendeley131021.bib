file = {:D$\backslash$:/mendeley/Cauraugh, Kim/Stroke/Cauraugh, Kim{\_}2002{\_}Two coupled motor recovery protocols are better than one Electromyogram-triggered neuromuscular stimulation and bilat.pdf:pdf},
isbn = {0000016926},
issn = {00392499},
journal = {Stroke},
keywords = {Cerebrovascular accident,Electromyography,Fingers,Motor activity,Muscle, skeletal,Transcutaneous electric nerve stimulation,Wrist},
number = {6},
pages = {1589--1594},
pmid = {12052996},
title = {{Two coupled motor recovery protocols are better than one: Electromyogram-triggered neuromuscular stimulation and bilateral movements}},
