doi = {10.1243/0954406JMES561},
file = {:D$\backslash$:/mendeley/Jackson et al/Proceedings of the Institution of Mechanical Engineers, Part C Journal of Mechanical Engineering Science/Jackson et al.{\_}2007{\_}Dual robot system for upper limb rehabilitation after stroke The design process.pdf:pdf},
issn = {09544062},
journal = {Proceedings of the Institution of Mechanical Engineers, Part C: Journal of Mechanical Engineering Science},
keywords = {Design,Rehabilitation,Robotic,Stakeholder analysis,Stroke,Upper limb,User involvement},
number = {7},
pages = {845--857},
title = {{Dual robot system for upper limb rehabilitation after stroke: The design process}},
