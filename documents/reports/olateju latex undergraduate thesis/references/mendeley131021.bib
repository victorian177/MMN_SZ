title = {{Methods for a Multisite Randomized Trial to Investigate the Effect of Constraint-Induced Movement Therapy in Improving Upper Extremity Function among Adults Recovering from a Cerebrovascular Stroke}},
url = {https://journals.sagepub.com/doi/abs/10.1177/0888439003255511},
