issn = {18754805},
journal = {International Journal of Social Robotics},
keywords = {Cooperative,Hierarchical fuzzy control architecture,Human-robot,Kinect,Rehabilitation},
month = {apr},
number = {2},
pages = {265--275},
publisher = {Springer Netherlands},
title = {{A Novel Human-Robot Cooperative Method for Upper Extremity Rehabilitation}},
url = {https://link.springer.com/article/10.1007/s12369-016-0393-4},
volume = {9},
