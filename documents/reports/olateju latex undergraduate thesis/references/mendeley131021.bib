journal = {Bulletin of Environmental Contamination and Toxicology},
number = {4},
pages = {465--472},
pmid = {107},
title = {{Potable water quality in rural Georgetown County}},
url = {http://rehabrobotics.org/icorr1999/papers/papers/lum.pdf},
volume = {14},
year = {1975}
}
@article{Spittle1991,
abstract = {This paper discusses the use of a new test apparatus and new testing methods for calibrating multi-axis load cells and minimizing off-axis crosstalk. The test apparatus was developed at the Crew Systems Directorate of the Armstrong Laboratory, Wright-Patterson Air Force Base, Ohio where multi-axis load cells are used in manikin testing to predict potential air crew member injury. The Denton Inc. six channel head/neck load cell was chosen for the tests because of its unique loading through the occipital condyle pin and nodding block surfaces and its importance in impact testing for providing essential neck loading data. Two load cells were used in the comparison study between the new test apparatus, the previous calibrating device - a prototype to the new fixture, and the load cell manufacturer to validate the current system and demonstrate its improved capabilities. The two load cells were calibrated on the previous fixture using the currently accepted method for that device. The same two load cells were then tested on the new fixture using four new calibration methods. The load cells were then calibrated by the manufacturer. The load cells were calibrated in all six axes for each test and data were collected on all channels during each axis calibration. These data were then reduced to 6 × 6 sensitivity matrices. The results from the different methods were compared and evaluated with particular assessment of off-axis crosstalk. The criteria for the best method was based upon the minimization of off-axis crosstalk and accuracy to full scale loading when the sensitivities and load sensitivity matrix are multiplied. The new test apparatus demonstrated improved capabilities.},
author = {Spittle, Eric K. and White, Richard P. and Kaleps, Ints},
journal = {Proceedings - Annual SAFE Symposium (Survival and Flight Equipment Association)},
keywords = {Acceleration Stresses (Physiology),Calibrating,Crosstalk,Dummies,Dynamic Loads,Flight Crews,Instrumentation and Photography,Neck (Anatomy),Safety Factors,Shear Stress,Test Equipment},
number = {29},
pages = {1--7},
title = {{New calibration methods for the denton six channel head/neck load cell. A comparative study}},
url = {https://ui.adsabs.harvard.edu/abs/1992safe.symp....1S/abstract},
year = {1991}
}
@article{Hijmans2011a,
abstract = {This study aimed to determine the effectiveness of a bilateral, self-supported, upper-limb rehabilitation intervention using a movement-based game controller for people with chronic stroke. Fourteen participants received a control treatment, followed by a washout period, and then the intervention. The intervention comprised playing computer games with the CyWee Z (CyWee Group Ltd; Taipei, Taiwan), a movement-based game controller similar to the Nintendo Wii remote. The CyWee Z was incorporated into a handlebar, making bilateral exercises possible by allowing the unaffected side to support and assist the affected side. The intervention lasted for 8 to 10 sessions of 45 to 60 minutes over a period of 2.5 weeks. The Fugl-Meyer Assessment upper-limb section (FMA-UL) was used as the primary outcome. The Wolf Motor Function Test and the Disabilities of Arm, Shoulder, and Hand outcome measure were used as secondary outcomes. Postintervention, motor performance as measured by the FMA-UL was significantly improved compared with all preintervention assessments (p {\textless} 0.001), whereas no changes were found on both secondary outcomes. It can be concluded from this pilot study that upper-limb motor performance of adults with chronic stroke improves with repetitive, game-assisted, self-supported bilateral exercises.},
