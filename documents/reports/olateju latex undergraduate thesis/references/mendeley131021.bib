file = {:D$\backslash$:/mendeley/Yun et al/International Journal of Precision Engineering and Manufacturing/Yun et al.{\_}2016{\_}Handling subject arm uncertainties for upper limb rehabilitation robot using robust sliding mode control.pdf:pdf},
issn = {20054602},
