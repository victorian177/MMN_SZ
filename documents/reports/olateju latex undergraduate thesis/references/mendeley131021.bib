issn = {17483107},
journal = {Disability and Rehabilitation: Assistive Technology},
keywords = {Stroke,bilateral,hemiparesis,physical therapy,rehabilitation,upper limb,virtual reality},
month = {jan},
number = {1},
pages = {55--62},
pmid = {21446826},
publisher = {Taylor {\&} Francis},
title = {{Bilateral upper limb trainer with virtual reality for post-stroke rehabilitation: Case series report}},
url = {https://www.tandfonline.com/doi/abs/10.3109/17483107.2011.562959},
