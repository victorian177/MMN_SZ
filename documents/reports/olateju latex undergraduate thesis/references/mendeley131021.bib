issn = {13507540},
keywords = {Motor,Plasticity,Stimulation,Stroke,TMS,tDCS},
number = {6},
pages = {667--674},
pmid = {16280678},
title = {{Drivers of brain plasticity}},
volume = {18},
year = {2005}
}
