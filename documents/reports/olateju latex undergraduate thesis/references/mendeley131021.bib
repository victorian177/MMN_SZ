issn = {20763417},
journal = {Applied Sciences (Switzerland)},
keywords = {Assist-as-needed,End-effector rehabilitation robot,Robot-aided training,Virtual channel},
month = {oct},
number = {19},
pages = {6684},
publisher = {MDPI AG},
title = {{Development and assist-as-needed control of an end-effector upper limb rehabilitation robot}},
