issn = {23146141},
journal = {BioMed Research International},
pmid = {27123448},
publisher = {Hindawi Limited},
title = {{tDCS and Robotics on Upper Limb Stroke Rehabilitation: Effect Modification by Stroke Duration and Type of Stroke}},
