file = {:D$\backslash$:/mendeley/Reinkensmeyer, Dewald, Rymer/IEEE Transactions on Rehabilitation Engineering/Reinkensmeyer, Dewald, Rymer{\_}1999{\_}Guidance-based quantification of arm impairment following brain injury A pilot study.pdf:pdf},
