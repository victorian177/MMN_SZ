keywords = {Gaming,Medical devices,Patient adherence,Rehabilitation,Rehabilitation engineering,Robotics,Stroke,Telerehabilitation},
month = {dec},
number = {4},
pages = {312--319},
publisher = {Springer},
title = {{The Present and Future of Robotic Technology in Rehabilitation}},
url = {https://link.springer.com/article/10.1007/s40141-016-0139-0},
volume = {4},
year = {2016}
}
