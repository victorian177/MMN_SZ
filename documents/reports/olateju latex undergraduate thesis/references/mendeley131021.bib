@article{Nadas2019,
abstract = {The paper addresses the upper limb rehabilitation for post-stroke patients. A dual-arm exoskeleton for the elbow and wrist flexion-extension is proposed. The added benefit of this robotic system is given by its portability and its ability to perform mirrored exercises using the motions provided by the unimpaired limb. The dual-arm exoskeleton addresses patients in the chronic phase after the stroke and even the patients in the post-acute phase allowing them to do rehabilitation exercises themselves.},
