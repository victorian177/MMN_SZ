keywords = {Bidirectional control,Bilateral arm training,Energy recycling,Force sensing,Master-slave motion tracking},
number = {1},
pages = {4198},
title = {{A NEW MASTER-SLAVE CONTROL METHOD FOR IMPLEMENTING FORCE SENSING AND ENERGY RECYCLING IN A BILATERAL ARM TRAINING ROBOT Chunguang Li , Yoshio Inoue , Tao Liu , Kyoko Shibata and Koichi Oka Received September 2009 ; Revised February 2010 1 . Introduction .}},
