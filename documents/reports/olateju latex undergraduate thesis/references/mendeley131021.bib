isbn = {1650197705},
issn = {16501977},
journal = {Journal of Rehabilitation Medicine},
keywords = {Arm rehabilitation,Robotics,Stroke},
number = {4},
pages = {310--314},
pmid = {20461332},
title = {{Arm studio to intensify upper limb rehabilitation after stroke: concept, acceptance, utilization and preliminary clinical results}},
